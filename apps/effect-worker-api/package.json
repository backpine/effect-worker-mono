{
  "name": "effect-worker-api",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest",
    "cf-typegen": "wrangler types --env-interface Env",
    "typecheck": "tsc --noEmit",
    "build": "tsc && wrangler deploy --dry-run",
    "db:push": "DATABASE_URL=postgres://postgres:postgres@localhost:5432/effect_worker drizzle-kit push",
    "db:studio": "DATABASE_URL=postgres://postgres:postgres@localhost:5432/effect_worker drizzle-kit studio"
  },
  "dependencies": {
    "@backpine/api": "workspace:^",
    "@backpine/domain": "workspace:^",
    "@effect/experimental": "latest",
    "@effect/platform": "latest",
    "@effect/sql": "latest",
    "@effect/sql-drizzle": "latest",
    "@effect/sql-pg": "latest",
    "drizzle-orm": "^0.45.0",
    "effect": "latest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^22.0.0",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "wrangler": "^4.55.0"
  }
}
