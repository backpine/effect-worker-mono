{
  "name": "@repo/cloudflare",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "Cloudflare Worker infrastructure for Effect",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@effect/experimental": "latest",
    "@effect/platform": "latest",
    "@effect/sql": "latest",
    "@effect/sql-drizzle": "latest",
    "@effect/sql-pg": "latest",
    "drizzle-orm": "^0.45.0",
    "effect": "latest"
  }
}
