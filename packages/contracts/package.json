{
  "name": "@backpine/contracts",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "API contract definitions for Effect Worker (HTTP and RPC)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "import": "./dist/http/index.js"
    },
    "./rpc": {
      "types": "./dist/rpc/index.d.ts",
      "import": "./dist/rpc/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@effect/platform": "latest",
    "@effect/rpc": "latest",
    "@backpine/cloudflare": "workspace:^",
    "@backpine/domain": "workspace:^",
    "effect": "latest"
  }
}
